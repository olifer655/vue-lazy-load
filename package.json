{
  "name": "vue-lazy-load",
  "version": "0.2.2",
  "description": "An infinite scroll directive for vue.js.",
  "main": "lib/vue-lazy-load.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --inline --config ./webpack.example.config.js --port",
    "remote-dev": "npm run dev --host",
    "build": "webpack -p --config webpack.example.config.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olifer655/vue-lazyload.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "lazy",
    "vue"
  ],
  "author": "ziran.liu@ele.me",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^1.0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.13.2",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "karma-babel-preprocessor": "^6.0.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.2.2",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^7.3.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "xo": "^0.12.1"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "space": true
